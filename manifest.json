{
  "manifest_version": 2,

  "name": "Passave",
  "description": "This extension shows a password of website ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },

  "background" : {
    "scripts" : ["eventPage.js"],
    "persistent" : false
  },

  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ]
}
